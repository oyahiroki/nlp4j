	<parent>
		<groupId>org.nlp4j</groupId>
		<artifactId>nlp4j</artifactId>
		<version>1.3.1.0</version>
	</parent>
	<!-- ARTIFACT ID -->
	<artifactId>nlp4j-core</artifactId>
	<!-- JAR -->
	<packaging>jar</packaging>
	<name>nlp4j-core</name>
